[user]
        name = {{ git_config_user_name }}
        email = {{ git_config_user_email }}
        signingkey = {{ git_config_user_signingkey }}
[branch]
        autoSetupRebase = always
[core]
        pager = less --no-init --quit-if-one-screen
[credential]
        helper = cache
[fetch]
        prune = true
        pruneTags = true
[merge]
        tool = vimdiff
[push]
        default = upstream
[rebase]
        autoSquash = true
        autoStash = true
[rerere]
        enabled = true
