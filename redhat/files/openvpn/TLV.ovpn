[connection]
id=Tel Aviv (TLV)
uuid=e396fe67-4bb0-426a-850c-e8786fae6cea
type=vpn

[vpn]
ca=/etc/ssl/certs/2015-RH-IT-Root-CA.pem
cipher=AES-256-CBC
connection-type=password
password-flags=2
port=443
remote=ovpn-tlv.redhat.com
reneg-seconds=0
verify-x509-name=name:ovpn.redhat.com
tunnel-mtu=1360
service-type=org.freedesktop.NetworkManager.openvpn

[ipv4]
dns-search=
method=auto
never-default=true

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
never-default=true
