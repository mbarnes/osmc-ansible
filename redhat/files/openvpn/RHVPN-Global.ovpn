[connection]
id=Red Hat Global
uuid=e69b124b-50a8-4458-8859-5e68c3f19460
type=vpn

[vpn]
ca=/etc/ssl/certs/2015-RH-IT-Root-CA.pem
cipher=AES-256-CBC
connection-type=password
password-flags=2
port=443
remote=ovpn.redhat.com
reneg-seconds=0
username=changeme
verify-x509-name=name:ovpn.redhat.com
tunnel-mtu=1360
service-type=org.freedesktop.NetworkManager.openvpn

[ipv4]
dns-search=
method=auto
never-default=true

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
never-default=true
